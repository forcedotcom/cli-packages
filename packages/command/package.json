{
  "name": "@salesforce/command",
  "version": "3.0.0",
  "description": "Salesforce CLI base command class",
  "main": "lib/exported",
  "repository": "https://github.com/forcedotcom/cli-packages",
  "license": "BSD-3-Clause",
  "scripts": {
    "build": "yarn sfdx-build",
    "clean": "yarn sfdx-clean",
    "clean-all": "yarn sfdx-clean all",
    "compile": "yarn sfdx-compile",
    "docs": "yarn sfdx-docs",
    "format": "yarn sfdx-format",
    "lint": "yarn sfdx-lint",
    "prepack": "yarn sfdx-build",
    "prepare": "npm run build",
    "test": "yarn sfdx-test"
  },
  "keywords": [
    "force",
    "salesforce",
    "sfdx",
    "salesforcedx",
    "sfdx-command"
  ],
  "files": [
    "lib",
    "messages",
    "!lib/**/*.map"
  ],
  "dependencies": {
    "@oclif/command": "^1.5.17",
    "@oclif/errors": "^1.2.2",
    "@oclif/parser": "^3.8.3",
    "@oclif/plugin-help": "^2.2.0",
    "@oclif/test": "^1.2.4",
    "@salesforce/core": "^2.6.0",
    "@salesforce/kit": "^1.2.2",
    "@salesforce/ts-types": "^1.2.0",
    "chalk": "^2.4.2",
    "cli-ux": "^4.9.3"
  },
  "devDependencies": {
    "@oclif/config": "^1.15.1",
    "@salesforce/dev-scripts": "^0.4.0",
    "@salesforce/ts-sinon": "^1.1.0",
    "shelljs": "~0.8.3"
  },
  "types": "lib/exported.d.ts",
  "publishConfig": {
    "access": "public"
  }
}
